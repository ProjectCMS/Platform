{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "artesaos/seotools": "^0.12.0",
    "chumper/zipper": "^1.0",
    "cyrildewit/eloquent-viewable": "^2.3",
    "davejamesmiller/laravel-breadcrumbs": "5.x",
    "fideloper/proxy": "^4.0",
    "genealabs/laravel-model-caching": "^0.2.62",
    "geoip2/geoip2": "^2.9",
    "guzzlehttp/guzzle": "^6.3",
    "hesto/multi-auth": "^2.0",
    "ilovepdf/ilovepdf-php": "^1.1",
    "jenssegers/date": "^3.3",
    "laravel/framework": "5.6.*",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.6",
    "mohd-isa/laravel-settings": "^2.2",
    "nwidart/laravel-modules": "^3.1",
    "pragmarx/tracker": "^3.3",
    "roumen/sitemap": "^2.8",
    "shipu/themevel": "^1.6",
    "spatie/laravel-permission": "^2.12"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "nunomaduro/collision": "~1.1",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/"
    },
    "files": [
      "Modules/Core/Helpers/modules.php",
      "Modules/Core/Helpers/nav-menus.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
